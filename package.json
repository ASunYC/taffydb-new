{
  "name": "@asun01/taffydb-new",
  "version": "1.0.0",
  "description": "taffydb-new is clone library from TaffyDB.",
  "keywords": [
    "database",
    "browser",
    "json",
    "collection",
    "records",
    "node",
    "nodejs"
  ],
  "main": "taffy.js",
  "scripts": {
    "test": "node_modules/.bin/nodeunit test/t.js",
    "build": "npm run clean && mkdir dist && npm run build:min && shx cp taffy.js taffy.min.js taffy-es.js package.json LICENSE README.md dist/",
    "clean": "shx rm -rf dist",
    "build:min": "uglifyjs taffy.js -o taffy.min.js",
    "pub": "cd dist && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/typicaljoe/taffydb.git"
  },
  "author": {
    "name": "asun01"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "license": "BSD-2-Clause",
  "homepage": "http://www.taffydb.com",
  "dependencies": {
    "@asun01/taffydb-new": "file:",
    "taffydb-new": "file:"
  },
  "devDependencies": {
    "jslint": "^0.12.1",
    "node-inspector": "^0.7.5",
    "nodeunit": "^0.10.2",
    "nodeunit-b": "^4.0.0",
    "shx": "^0.3.4",
    "uglify-js": "^3.19.2"
  }
}
